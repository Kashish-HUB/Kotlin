class introduction{
    var name : String;
    var age:Int;
    constructor( x:String, y:Int){
        this.name=x;
        this.age=y;
    }
    constructor( x:String){
        this.name=x;
        this.age=0;
    }
    constructor(){
        this.name="Kashish"
        this.age=20
    }
    fun intro(){
        println("My name is $name and age is $age")
    }
}
fun main(){
    var k=introduction()
    k.intro()
}
