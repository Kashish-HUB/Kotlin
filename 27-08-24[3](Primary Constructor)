
class Introduction(var name:String,var age:Int){

    constructor( x:String):this(){
        this.name=x
        this.age=0
    }
    constructor():this("Kashish",20)

    fun intro(){
        println("My name is $name and age is $age")
    }
}
fun main(){
    var k=Introduction("Dupinder",25)
    var l=Introduction("Sukhwinder",28)
    k.intro()
    l.intro()
}
